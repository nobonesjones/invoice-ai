{
  "expo": {
    "name": "SuperInvoice",
    "slug": "meetings",
    "scheme": "superinvoice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/superinvoiceicon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": false,
    "jsEngine": "jsc",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-av",
      "expo-video",
      "expo-web-browser",
      "expo-apple-authentication",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to allow you to choose a logo for your business.",
          "cameraPermission": "The app accesses your camera to allow you to take a photo for your business logo."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "The app accesses your contacts to allow you to quickly add clients from your address book."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone to use voice commands for creating invoices and entering information",
        "NSContactsUsageDescription": "The app accesses your contacts to allow you to quickly add clients from your address book.",
        "UIBackgroundModes": [
          "audio",
          "audio"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1086610940828-pidi39sius8h03f7rgm0vp0gauu0fo4j",
              "superinvoice"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1086610940828-pidi39sius8h03f7rgm0vp0gauu0fo4j",
              "superinvoice"
            ]
          }
        ],
        "NSLocalNetworkUsageDescription": "Allow local network to connect to the development server during debugging.",
        "NSBonjourServices": [
          "_expo-development-server._tcp",
          "_expo-development-client._tcp",
          "_http._tcp",
          "_https._tcp"
        ]
      },
      "splash": {
        "image": "./assets/superinvoiceicon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/superinvoiceicon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "bundleIdentifier": "com.indianabones.ExpoSupabaseStarter"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.indianabones.ExpoSupabaseStarter",
      "permissions": [
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "READ_CONTACTS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_CONTACTS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "splash": {
        "image": "./assets/superinvoiceicon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/superinvoiceicon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/d275f575-073b-4bde-8867-4d9b7387cc36",
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d275f575-073b-4bde-8867-4d9b7387cc36"
      },
      "EXPO_PUBLIC_GEMINI_API_KEY": ""
    },
    "owner": "indianabones",
    "runtimeVersion": "1.0.0"
  }
}
