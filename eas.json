{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true
    },
    "jsc": {
      "distribution": "store",
      "env": {
        "EXPO_JS_ENGINE": "jsc"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
